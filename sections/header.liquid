                                <div class="header__btn-language position-relative d-none d-lg-flex align-items-lg-center mt-lg-4 cursor-pointer" data-js-dropdown-button>
                                    {%- for locale in shop.published_locales -%}
                                        {%- if locale.iso_code == shop.locale -%}
                                            <span>{{ locale.name }}</span>
                                            <i>{% render 'icon-theme-229' %}</i>
                                        {%- endif -%}
                                        {% capture locale_list_item_html %}
                                            <li{% if locale.iso_code == shop.locale %} class="active"{% endif %}>
                                                {%- if locale.iso_code == shop.locale -%}
                                                    <span>{{ locale.name }}</span>
                                                {%- else -%}
                                                    <a href="{{ locale.root_url }}">{{ locale.name }}</a>
                                                {%- endif -%}
                                            </li>
                                        {% endcapture %}
                                        {%- if locale.primary -%}
                                            {% capture locale_list_primary_item_html %}
                                                {{ locale_list_item_html }}
                                            {% endcapture %}
                                        {%- else -%}
                                            {% capture locale_list_items_html %}
                                                {{ locale_list_items_html }}
                                                {{ locale_list_item_html }}
                                            {% endcapture %}
                                        {%- endif -%}
                                    {%- endfor -%}
                                </div>
                                <div class="header__dropdown dropdown d-lg-none position-lg-absolute top-lg-100 right-lg-0" data-js-dropdown>
                                    <ul class="list-unstyled w-100 px-15 pb-30 py-lg-15">
                                        {{ locale_list_primary_item_html }}
                                        {{ locale_list_items_html }}
                                    </ul>
                                </div>
                            </div>
                        {% when 'weglot' %}
