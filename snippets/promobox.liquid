        {%- when 'preset_1' -%}
            {%- assign responsive_height = promobox_height_preset_1 -%}
            {%- assign ultra_responsive_height = promobox_ultra_height_preset_1 -%}
        {%- when 'preset_2' -%}
            {%- assign responsive_height = promobox_height_preset_2 -%}
